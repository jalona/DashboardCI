	"type":         "symfony-bundle",
	"license":      "MIT",
	"authors": [
		{
			"name":     "MB-WEBDEV",
			"email":    "michael@mb-webdev.ch"
		}
	],
	"require": {
		"php":                               ">=5.3.3",
		"symfony/framework-bundle":          "~2.3",
		"symfony/twig-bundle":               "~2.3"
	},
	"require-dev": {
		"phpunit/phpunit": "~4"
	},
	"autoload": {
		"psr-0": { "MB\\DashboardBundle": "" }
