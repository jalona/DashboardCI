{
	"name":         "mb-webdev/dashboard-bundle",
	"description":  "MBDashboardBundle is a Symfony 3 bundle to communicate with GitLab, GitLab-CI, Stash and Bamboo",
	"keywords":     ["mb-webdev", "github", "gitlab", "stash", "gitlab-ci", "bamboo", "dashboard"],
	"type":         "symfony-bundle",
	"license":      "MIT",
	"authors": [
		{
			"name":     "MB-WEBDEV",
			"email":    "michael@mb-webdev.ch"
		}
	],
	"require": {
		"php": ">=5.5.9",
		"symfony/symfony": "3.0.*",
		"doctrine/orm": "^2.5",
		"doctrine/doctrine-bundle": "^1.6",
		"sensio/framework-extra-bundle": "^3.0.2"
	},
	"require-dev": {
		"phpunit/phpunit": "~4"
	},
	"autoload": {
		"psr-0": { "MB\\DashboardBundle": "" }
	},
	"target-dir": "MB/DashboardBundle"
}
